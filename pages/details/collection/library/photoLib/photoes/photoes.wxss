.hasNoPhoto{
  width: 400rpx;
  height: 400rpx;
  margin: 50% auto;
}
.add{
  text-align: center;
  font-size: 150rpx;
  color: #409eff;
  display: inline-block;
  float: left;
  width: 200rpx;
  height: 200rpx;
  margin-left: 20rpx;
  margin-top: 20rpx;
  padding: 10rpx;
  border: 2rpx dashed #409eff;
}
.empty{
  display: inline-block; 
  width: 400rpx;
  height: 200rpx;
  margin: auto;
  font-size: 32rpx;
  text-align: center;
}

/*相片样式  */
.baseline{
  float: left;
  width: 100%;
  text-align: center;
}
.photoBlock{
  width: 200rpx;
  height: 200rpx;
  margin-left: 20rpx;
  margin-bottom: 10rpx;
  margin-top: 20rpx;
  padding: 10rpx;
  float: left;
  border: 2rpx solid #eee;
  position: relative;
}
.photo{
  width: 200rpx;
  height: 190rpx;
}
