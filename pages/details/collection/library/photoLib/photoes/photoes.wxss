.hasNoPhoto{
  width: 400rpx;
  height: 400rpx;
  display: flex;
  align-content: center;
  align-items: center;
  justify-content: center;
  margin: 50% auto;
}
.add{
  text-align: center;
  font-size: 150rpx;
  color: #409eff;
  display: flex;
  align-content: center;
  align-items: center;
  justify-content: center;
  float: left;
  width: 200rpx;
  height: 200rpx;
  margin-left: 20rpx;
  margin-top: 20rpx;
  padding: 10rpx;
  border-radius: 20rpx;
  border: 2rpx dashed #409eff;
}
.empty{
  display: flex;
  align-content: center;
  align-items: center;
  justify-content: center; 
  width: 400rpx;
  height: 200rpx;
  margin: auto;
  font-size: 32rpx;
  text-align: center;
}

/*相片样式  */
.baseline{
  float: left;
  width: 100%;
  text-align: center;
}
.photoBlock{
  width: 33%;
  /* margin-left: 20rpx; */
  margin-bottom: 10rpx;
  margin-top: 20rpx;
  /* padding: 10rpx; */
  float: left;
  position: relative;
  border-radius: 10rpx;
}
.photo{
  /* width: 200rpx; */
  /* height: 200rpx; */
  width: 100%;
  height: auto;
  border-radius: 10rpx;
}
