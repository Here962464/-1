.fileBlocks{
  width: 100%;
  padding: 10rpx 0;
}
.folder{
  width: 100%;
  padding: 20rpx;
  border-bottom:2rpx solid #eee;
  display: flex;
  align-content: center;
  align-items: center;
  font-size: 32rpx;
}
.folderImg{
  width: 100rpx;
  height: 100rpx;
  margin-right: 20rpx;
}
.fileName{
  width:550rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.createDate{
  padding: 10rpx 0;
  font-size: 24rpx;
  color: #aaa;
}
.baseline{
  text-align: center;
  padding: 20rpx 0;
}
/*弹出层  */
.checkBox{
  padding-top: 30rpx;
}
.inputFileName{
  color: #000;
}